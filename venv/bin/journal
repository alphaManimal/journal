#!/Users/chadhokama/Dropbox/code/journal/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'journal','console_scripts','journal'
__requires__ = 'journal'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('journal', 'console_scripts', 'journal')()
    )
